    <script src="assets/tokyo/script.js"></script>
    <script>
        // Overlay button handlers for Line redirect and calling a number.
        (function(){
            function callNumber(number){
                // Attempt native call on mobile
                window.location.href = 'tel:' + number;
            }

            document.addEventListener('DOMContentLoaded', function(){
                var lineBtn = document.querySelector('.overlay-button.button-line');
                var callBtn = document.querySelector('.overlay-button.button-call');

                if(lineBtn){
                    lineBtn.addEventListener('click', function(e){
                        e.preventDefault();
                        e.stopPropagation();
                        // Replace the URL below with your LINE link or desired URL
                        var lineUrl = 'https://liff.line.me/2007384640-yj8nm8g3/landing?follow=%40931ezxtl&lp=O70xEW&liff_id=2007384640-yj8nm8g3';
                        window.open(lineUrl, '_blank');
                    });
                }

                if(callBtn){
                    callBtn.addEventListener('click', function(e){
                        e.preventDefault();
                        e.stopPropagation();
                        // Replace with the phone number you want to call (international format recommended)
                        var phoneNumber = '+810362625779';
                        callNumber(phoneNumber);
                    });
                }
            });
        })();
    </script>
</body>
</html>